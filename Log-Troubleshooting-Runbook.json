                      "data_source": "metrics",
                      "name": "query1",
                      "query": "avg:datadog.estimated_usage.logs.ingested_bytes{*} by {datadog_index,service}"
                    },
                    {
                      "data_source": "metrics",
                      "name": "query2",
                      "query": "sum:datadog.estimated_usage.logs.ingested_events{*} by {datadog_index,datadog_is_excluded,service,status}.as_count()"
